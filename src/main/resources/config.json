{
  "reader": {
    "class": "codeFormatter.writer.Reader",
    "param": {
      "file": "format.java"
    }
  },
  "reader-strategy": {
    "class": "codeFormatter.strategy.SignReaderStrategy"
  },
  "writer": {
    "class": "codeFormatter.writer.Writer",
    "param": {
      "file": "output.java"
    }
  },
  "formatter": {
    "class": "codeFormatter.JSCodeFormatter"
  },
  "handlers": [
    {
      "key"  : "{",
      "class": "codeFormatter.handlers.OpenBraceHandler"
    },
    {
      "key"  : "default",
      "class": "codeFormatter.handlers.Default"
    }
  ],
  "context": {
    "class": "codeFormatter.context.PermanentContext",
    "params": {
      "open-brace": "{"
    }
  }
}